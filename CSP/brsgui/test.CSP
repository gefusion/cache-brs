<html>
	<head>
		<title>	Страница администратора</title>
		<link rel="stylesheet" type="text/css" href="/brsgui/css/bootstrap.css">
		<link rel="stylesheet" type="text/css" href="/brsgui/css/custom.css">
		<link rel="stylesheet" type="text/css" href="/brsgui/css/font-awesome.min.css">
		<link rel="stylesheet" type="text/css" href="/brsgui/css/bootstrap-select.min.css">
	</head>
		<body ng-controller="ctrl">
		<div class="container-fluid">
			<input ng-model="deanery" name="deanery" type="hidden" value="Users.DeaneryWorker"/>
			<input ng-model="faculty" name="faculty" type="hidden" value="Data.Faculty"/>
			<input ng-model="semester" name="semester" type="hidden" value="Data.Semester"/>
			<input ng-model="acadYear" name="acadYear" type="hidden" value="Data.AcademicYear"/>
	        <header class="ng-isolate-scope">
	        	<nav class="navbar navbar-default">
					<div class="navbar-header">
						<a class="navbar-brand ng-binding">Страница администратора</a>
					</div>
					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
						<ul class="nav navbar-nav"> 
							<li class="active"><a  data-toggle="tab" href="#facultiesList" >Управление факультетами</a></li>
							<li><a data-toggle="tab" href="#academicYearsList">Управление учебными годами</a></li>
							<li><a data-toggle="tab" href="#semestersList">Управление семестрами</a></li>
							<li><a data-toggle="tab" href="#deaneryWorkersList">Управление сотрудниками деканатов</a></li>
							<li><a href="#("/brs/logout")#">Выйти</a></li>
	      				</ul>
	      				<h5 class="user-info">
	      					Пользователь: #($username)#<br>
						</h5>
	      			</div>
	      		</nav>
	    	</header>
	    	<div class="tab-content">
	    		<csp:include page="/brsgui/partials/faculties.CSP">
	    		<csp:include page="/brsgui/partials/academicYears.CSP">
	    		<csp:include page="/brsgui/partials/semesters.CSP">
	    		<csp:include page="/brsgui/partials/deaneryWorkers.CSP">
	    	</div>
		    	<csp:include page="/brsgui/partials/personForm.CSP">
		    	<csp:include page="/brsgui/partials/deleteModal.CSP">
	    </div>
	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular.min.js"></script>
	<script type="text/javascript" src="/brsgui/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="/brsgui/js/lib/bootstrap-select.min.js"></script>
	<script type="text/javascript" src="/brsgui/js/ui-source.js"></script>
	<script type="text/javascript" src="/brsgui/js/ui-grid.js"></script>
	<script type="text/javascript" src="/brsgui/js/admin.js"></script>
	</body>
	</html>